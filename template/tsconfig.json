{
	"compilerOptions": {
		"target": "esnext",
		"lib": ["ES2017"],
		"allowJs": true,
		"jsx": "react",
		"noEmit": true,
		"isolatedModules": false,
		"strict": true,
		"moduleResolution": "node",
		"baseUrl": "./src",
		"paths": {"*": ["./*"]},
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"resolveJsonModule": true
	},
	"exclude": [
		"node_modules",
		"babel.config.js",
		"metro.config.js",
		"react-native.config.js",
		"jest.config.js"
	]
}